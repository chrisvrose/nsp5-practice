    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc - defclient",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/defclient",
            "args": ["localhost"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
